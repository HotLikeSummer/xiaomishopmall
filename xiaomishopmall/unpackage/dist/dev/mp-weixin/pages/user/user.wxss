
._li {
	list-style: none;
}
.top {
	width: 100%;
	height: 400rpx;
	background-image: url("/static/images/bg.jpg");
	background-repeat: no-repeat;
	background-size: cover;
	position: relative;
}
.top image {
	width: 180rpx;
	height: 180rpx;
	border-radius: 50%;
	margin-left: 65rpx;
	margin-top: 130rpx;
}
.icon-xiaoxi {
	color: white;
	font-size: 60rpx;
	float: right;
	margin-top: 50rpx;
	margin-right: 30rpx;
}
.member {
	color: #C94B00;
	width: 300rpx;
	height: 80rpx;
	line-height: 80rpx;
	background-color: #FFD43F;
	border-top-left-radius: 40rpx;
	border-bottom-left-radius: 40rpx;
	float: right;
	text-align: center;
	position: absolute;
	bottom: 80rpx;
	right: 0px;
}
.icon-huangguan {
	margin-right: 15rpx;
}
.order-tit {
	height: 100rpx;
	line-height: 100rpx;
	padding: 0px 20rpx;
	border-bottom: 1px solid #f0f0f0;
}
.lt-order {
	font-weight: 600;
	font-size: 35rpx;
	color: #393D43;
}
.rt-order {
	float: right;
	color: #8C949B;
	height: 100rpx;
	line-height: 100rpx;
}
.waite {
	width: 100%;
	height: 120rpx;
	line-height: 40rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-justify-content: space-around;
	    -ms-flex-pack: distribute;
	        justify-content: space-around;
	margin-top: 30rpx;
}
.waite-list {
	text-align: center;
	margin-bottom: 30rpx;
}
.line {
	width: 100%;
	height: 20rpx;
	background-color: #F5F5F5;
}
.advertising image {
	width: 100%;
	height: 300rpx;
}
.servilist{
	padding: 0px 30rpx;
	height: 80rpx;
	line-height: 80rpx;
	border-bottom: 1px solid #f0f0f0;
}
.servilist:last-child{
	border-top: 20rpx solid #F5F5F5;
	border-bottom: none;
}
.rightTo{
	color: #C2C2C2;
	float: right;
}
.content{
	margin-left: 20rpx;
}
.icon-VIP{
	color: #FDC43E;
}
.servilist .icon-huangguan{
	color: #FFD43F;
	margin-right: 0rpx;
}
.icon-service{
	color: #FA6C5E;
}
.icon-home{
	color: #FE8539;
}
.icon-gengduo{
	color: #9CD458;
}
.icon-icon_set_up{
	color: #7E8A97;
}

