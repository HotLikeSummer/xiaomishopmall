
._li{
	list-style: none;
}
#editProfile {
	position: fixed;
	width: 100%;
	height: 100%;
	background-color: #EEEEEE;
	font-size: 60rpx;
}
.box {
	height: 20rpx;
}
.head {
	height: 150rpx;
	line-height: 150rpx;
	width: 100%;
	background-color: white;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	border-bottom: 1rpx solid #E0E0E0;
}
.head image {
	display: block;
	width: 100rpx;
	height: 100rpx;
	border-radius: 50%;
	margin-top: 25rpx;
	margin-right: 30rpx;
}
.head text {
	margin-left: 30rpx;
}
.name{
	height: 120rpx;
	line-height: 120rpx;
	width: 100%;
	background-color: white;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	border-bottom: 1rpx solid #E0E0E0;
}
.content{
	height: 120rpx;
	line-height: 120rpx;
}
.name .icon-you{
	margin-left: 10rpx;
	color: #D1D1D1;
}
.edit-item{
	height: 120rpx;
	line-height: 120rpx;
	width: 100%;
}
.edit-list{
	background-color: white;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	padding: 0rpx 30rpx;
	border-bottom: 1rpx solid #E0E0E0;
}
.edit-list .icon-you{
	color: #D1D1D1;
}
.edit-list:nth-child(2){
	border-bottom:none;
	margin-bottom: 20rpx;
}

